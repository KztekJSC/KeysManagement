@model Kztek_Model.Models.Customer

@await Html.PartialAsync("Partial_Action", new Kztek_Library.Models.PartialActionModel<Kztek_Model.Models.Customer>()
{
    Model = Model,
    Controller = "Customer",
    Action = "Create",
    Page = 1
})
@section scripts{
    <script>
        $(document).ready(function () {
            $("#Countinue").click(function () {
                $("#SaveAndCountinue").val('true');
            });

            $("#BackToList").click(function () {
                $("#SaveAndCountinue").val('false');
            });
        });
    </script>
}


